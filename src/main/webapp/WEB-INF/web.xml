<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file></welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>Import</servlet-name>
    <servlet-class>Import</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Import</servlet-name>
    <url-pattern>/import_references</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>main</servlet-name>
    <jsp-file>/page_1/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>main</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>importlist</servlet-name>
    <jsp-file>/page_2/importlist.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>importlist</servlet-name>
    <url-pattern>/import_list</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>calculated</servlet-name>
    <jsp-file>/page_4/calculated.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>calculated</servlet-name>
    <url-pattern>/calculated</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>Transit</servlet-name>
    <servlet-class>Transit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Transit</servlet-name>
    <url-pattern>/transit</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AnalogImport</servlet-name>
    <jsp-file>/page_3/analogimport.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>AnalogImport</servlet-name>
    <url-pattern>/import_of_analogs</url-pattern>
  </servlet-mapping>
</web-app>
